<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/Header_PTSD26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PTSD - Car Accident"
        android:textSize="35sp"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:padding="16dp"/>

    <TextView
        android:id="@+id/Header2_PTSD26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Session 2-6: Gradual Exposure"
        android:textSize="28sp"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@+id/Header_PTSD26"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:padding="16dp"/>

    <TextView
        android:id="@+id/b1_PTSD26"
        android:layout_width="386dp"
        android:layout_height="48dp"
        android:padding="12dp"
        android:text="Session 2: Begin with Car Driving Experience"
        android:textColor="@color/black"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Header2_PTSD26" />

    <TextView
        android:id="@+id/b2_PTSD26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="https://youtu.be/DhE9J_XbeK4?si=wAzlNO6cp2T_1Oku"
        android:textSize="15sp"
        android:textColor="@color/black"
        android:autoLink="web"
        android:linksClickable="true"
        app:layout_constraintTop_toBottomOf="@+id/b1_PTSD26"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:padding="12dp"/>

    <TextView
        android:id="@+id/b3_PTSD26"
        android:layout_width="398dp"
        android:layout_height="46dp"
        android:autoLink="web"
        android:linksClickable="true"
        android:padding="12dp"
        android:text="Session 3: Continue with Car Blind Spot training"
        android:textColor="@color/black"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b2_PTSD26" />

    <TextView
        android:id="@+id/b4_PTSD26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="https://youtu.be/Re6GEscZY1I?si=MoRpvyRxcHNHWg5i"
        android:textSize="15sp"
        android:textColor="@color/black"
        android:autoLink="web"
        android:linksClickable="true"
        app:layout_constraintTop_toBottomOf="@+id/b3_PTSD26"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:padding="12dp"/>

    <TextView
        android:id="@+id/b5_PTSD26"
        android:layout_width="394dp"
        android:layout_height="69dp"
        android:autoLink="web"
        android:linksClickable="true"
        android:padding="12dp"
        android:text="Session 4: Continue with Car Stuck on train track scenario"
        android:textColor="@color/black"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b4_PTSD26" />

    <TextView
        android:id="@+id/b6_PTSD26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="https://youtu.be/lwRaubd7TYw?si=j9b7kit7AtOHC3mo"
        android:textSize="15sp"
        android:textColor="@color/black"
        android:autoLink="web"
        android:linksClickable="true"
        app:layout_constraintTop_toBottomOf="@+id/b5_PTSD26"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:padding="12dp"/>

    <TextView
        android:id="@+id/b7_PTSD26"
        android:layout_width="394dp"
        android:layout_height="42dp"
        android:autoLink="web"
        android:linksClickable="true"
        android:padding="12dp"
        android:text="Session 5: Continue with Car catching fire"
        android:textColor="@color/black"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b6_PTSD26" />

    <TextView
        android:id="@+id/b8_PTSD26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="https://youtu.be/9CIfntMu_Gs?si=1QH0hfeam-bCo8vE"
        android:textSize="15sp"
        android:textColor="@color/black"
        android:autoLink="web"
        android:linksClickable="true"
        app:layout_constraintTop_toBottomOf="@+id/b7_PTSD26"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:padding="12dp"/>

    <TextView
        android:id="@+id/b9_PTSD26"
        android:layout_width="393dp"
        android:layout_height="61dp"
        android:autoLink="web"
        android:linksClickable="true"
        android:padding="12dp"
        android:text="Session 6: Conclude gradual exposure with Car driving in tornado"
        android:textColor="@color/black"
        android:textSize="15sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b8_PTSD26" />

    <TextView
        android:id="@+id/b10_PTSD26"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="https://youtu.be/DJEjeKD-Jb8?si=VB27YdbPsk_HK5IB"
        android:textSize="15sp"
        android:textColor="@color/black"
        android:autoLink="web"
        android:linksClickable="true"
        app:layout_constraintTop_toBottomOf="@+id/b9_PTSD26"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:padding="12dp"/>

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/my_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:layout_alignParentBottom="true"
        android:background="@color/purple_500"
        android:elevation="4dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>